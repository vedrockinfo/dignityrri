{
  "name": "dignityrri",
  "version": "1.0.0",
  "description": "Dignity RRI",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/viewbox.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedrockinfo/dignityrri.git"
  },
  "author": "Vedant Frontend Developer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vedrockinfo/dignityrri/issues"
  },
  "homepage": "https://github.com/vedrockinfo/dignityrri#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "sass": "^1.83.0"
  }
}
